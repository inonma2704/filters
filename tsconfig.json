{
  "compilerOptions": {
    "target": "ES2015",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "incremental": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "strictPropertyInitialization": false,
    "preserveWatchOutput": true,
    "declaration": false,
    "sourceMap": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "files": [
    "reset.d.ts"
  ],
  "include": [
    "src/**/**/*.tsx",
    "src/**/**/*.ts",
    "reset.d.ts"
  ],
  "exclude": [
    "**/node_modules/*",
    "node_modules"
  ]
}